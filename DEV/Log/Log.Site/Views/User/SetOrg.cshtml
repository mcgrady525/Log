<div style="padding: 15px; overflow: hidden">
    <form id="ui_user_setorgform" method="post" action="">
        <table class="tableForm" width="100%">
            <tr>
                <th>
                    用户：
                </th>
                <td>
                    <input type="hidden" id="UserIds" name="UserIds" />
                    <input type="text" id="UserNames" name="UserNames" style="width: 170px;"
                           disabled="disabled" />
                </td>
            </tr>
            <tr>
                <th>
                    机构：
                </th>
                <td>
                    <input class="easyui-combotree" id="CombotreeOrg" name="CombotreeOrg"
                           style="width: 180px;" data-options="url: '@Url.Action("GetTreeOrgData", "Organization")',cascadeCheck:false,multiple:true,lines:true" />
                    <img style="cursor: pointer; vertical-align: middle;" onclick="$('#OrgIds').combotree('clear');"
                         alt="清空机构" title="清空机构" src='@Url.Content("~/Content/easyui/icon/edit-clear.png")' />
                </td>
            </tr>
        </table>
    </form>
</div>
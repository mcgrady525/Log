<configuration>
  <configSections>
    <section name="Paths" type="Tracy.Frameworks.Configurations.PathsConfigurationSection,Tracy.Frameworks.Configurations" />
  </configSections>
  
  <!--Paths-->
  <Paths configSource="VConfigs\Debug\Paths.config" />
  
  <connectionStrings configSource="VConfigs\Debug\connectionStrings.config" />
  <appSettings configSource="VConfigs\Debug\appSettings.config" />
